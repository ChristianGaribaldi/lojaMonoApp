<div class="container-fluid">
  <!--<div class="row">-->
    <div class="col-md-10 col-md-offset-1">
      <div class="page-header">
        <h3><strong>Efetuar Venda</strong></h3>
      </div>

      <div class="form-group form-inline">
        <label> Cliente</label>
        <select class="form-control">
          <option> Christian</option>
        </select>
      </div>

      <div class="col-md-6">
        <!--PANEL DE PRODUTOS EM ESTOQUE-->
        <div class="panel panel-primary">
          <!-- Default panel contents -->
          <div class="panel-heading">Produtos em Estoque</div>
          <!-- Table -->
          <table class="table">
            <thead>
            <tr>
              <th>Nome</th>
              <th>Unidade(R$)</th>
              <th>Estoque</th>
              <th colspan="1"></th>
            </tr>
            </thead>

            <tbody>
            <tr class="linha">
              <td>ASDASDA </td>
              <td>R$ 29,00 </td>
              <td>150 </td>
              <td>
                <button class="btn btn-success">+</button>
              </td>
            </tr>
            </tbody>
          </table>
        </div>
        <!--FIM PANEL DE PRODUTOS EM ESTOQUE-->
      </div>

      <div class="col-md-6">
        <!--2 Panel de Carrinho-->
        <div class="panel panel-success">
          <!-- Default panel contents -->
          <div class="panel-heading"  style="background-color: #67b168">Carrinho</div>
          <!-- Table -->
          <table class="table">
            <thead>
            <tr>
              <th>Nome</th>
              <th>Unidade(R$)</th>
              <th>Quantidade</th>
              <th colspan="1"></th>
            </tr>
            </thead>

            <tbody>
            <tr class="linha">
              <td>ASDASDA </td>
              <td>R$ 29,00 </td>
              <td>
              <span class="col-md-10">
                <input type="number" class="form-control" min="1" step="1"/>
              </span>
              </td>
              <td><button class="btn btn-danger">-</button> </td>
            </tr>
            </tbody>
          </table>
        </div>
        <!-- FIM Panel de Carrinho-->
      </div>

      <div class="well col-md-12">
        <div class="col-md-6">
          <div class="h5"><strong>Forma de Pagamento:</strong>
            <label class="radio-inline">
              <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked>
              Dinheiro
            </label>
            <label class="radio-inline">
              <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked>
              Cartão
            </label>
          </div>
          <!-- Dados do Cartão-->
          <div class="input-group col-md-10">
            <span class="input-group-addon" id="basic-addon1">Número </span>
            <input type="text" class="form-control" placeholder="00000000000" aria-describedby="basic-addon1">
          </div>
          <div class="input-group col-md-10">
            <span class="input-group-addon" id="basic-addon1">Validade </span>
            <input type="text" class="form-control" placeholder="00/00" aria-describedby="basic-addon1">

            <span class="input-group-addon" id="basic-addon1">CVV </span>
            <input type="text" class="form-control" placeholder="000" aria-describedby="basic-addon1">
          </div>
          <!-- Fim Dados do Cartão-->
        </div>

        <div class="col-md-6">
          <div class="input-group col-md-6">
            <span class="input-group-addon" id="basic-addon1">Desconto %</span>
            <input type="number" class="form-control" placeholder="0" min="0" max="100" step="10" aria-describedby="basic-addon1">
          </div>
          <div class="h3">
            <strong> Valor Total: </strong> R$ 150
          </div>
        </div>

      </div>
      <button class="btn btn-primary btn-block">Concluir Compra</button>
    </div>
  </div>
<!--</div>-->
